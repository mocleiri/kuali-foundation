<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

  <property name="sql.encoding" value="UTF-8" />
  <property name="sql.delimiter" value="\n/\s*\n|\n/\s*$" />

  <changeSet id="initial-db" author="rice-liquibase" context="master">
    <!-- Oracle resources -->
    <ext:sqlResources dbms="oracle" location="classpath:META-INF/org/kuali/rice/rice-sql/oracle/schema.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />
    <ext:sqlResources dbms="oracle" location="classpath:META-INF/org/kuali/rice/rice-sql/oracle/data.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />
    <ext:sqlResources dbms="oracle" location="classpath:META-INF/org/kuali/rice/rice-sql/oracle/constraints.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />
    <ext:sqlResources dbms="oracle" location="classpath:META-INF/org/kuali/rice/rice-sql/oracle/other.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />

    <!-- MySQL resources -->
    <ext:sqlResources dbms="mysql" location="classpath:META-INF/org/kuali/rice/rice-sql/mysql/schema.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />
    <ext:sqlResources dbms="mysql" location="classpath:META-INF/org/kuali/rice/rice-sql/mysql/data.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />
    <ext:sqlResources dbms="mysql" location="classpath:META-INF/org/kuali/rice/rice-sql/mysql/constraints.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />
    <ext:sqlResources dbms="mysql" location="classpath:META-INF/org/kuali/rice/rice-sql/mysql/other.resources" endDelimiter="${sql.delimiter}" encoding="${sql.encoding}" />
  </changeSet>

</databaseChangeLog>