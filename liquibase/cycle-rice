#
# Pass in the target database type eg "oracle" or "mysql"
#

DB=$1
mvn process-resources -f pom-export.xml -Price,$DB -o
mvn process-resources -f pom-sql-gen.xml -Price,$DB -o
mvn process-resources -f pom-sql-exec.xml -Price,$DB -o
mvn process-resources -f pom-export.xml -Price,$DB -o -Dsql.username=LIQUIBASE -Dsql.password=LIQUIBASE -Dliquibase.changeLogDir=target/$DB/rice-2.0.0-m7/lb
