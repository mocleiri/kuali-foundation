<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.kuali.pom</groupId>
    <artifactId>kuali-common</artifactId>
    <version>3.3.43</version>
  </parent>
  <groupId>org.kuali.common</groupId>
  <artifactId>kuali-devops</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <name>DevOps Tooling</name>
  <description>DevOps related tooling</description>
  <url>http://${kuali.site.hostname}/common/${project.artifactId}/${project.version}</url>
  <scm>
    <connection>scm:${project.scm.vendor}:https://svn.kuali.org/repos/foundation/trunk/kuali-devops</connection>
    <developerConnection>scm:${project.scm.vendor}:https://svn.kuali.org/repos/foundation/trunk/kuali-devops</developerConnection>
    <url>https://svn.kuali.org/repos/foundation/trunk/kuali-devops</url>
  </scm>
  <properties>
    <kuali-aws.version>1.0.0-SNAPSHOT</kuali-aws.version>
    <kuali-dns.version>1.0.0-SNAPSHOT</kuali-dns.version>
    <kuali-util.version>4.4.9-SNAPSHOT</kuali-util.version>
    <kuali-http.version>1.0.8</kuali-http.version>
    <junit.version>4.11</junit.version>
    <hibernate-validator.version>5.0.1.Final</hibernate-validator.version>
    <javax.el.version>2.2.4</javax.el.version>
    <javax.el-api.version>${javax.el.version}</javax.el-api.version>
    <jsr305.version>2.0.3</jsr305.version>

    <tomcat7.version>7.0.47</tomcat7.version>
    <tomcat6.version>6.0.37</tomcat6.version>

    <tomcat.groupId>org.apache.tomcat</tomcat.groupId>
    <tomcat.artifactId>apache-tomcat</tomcat.artifactId>
    <tomcat.version>${tomcat7.version}</tomcat.version>
    <tomcat.type>zip</tomcat.type>

    <jdk7.groupId>com.oracle</jdk7.groupId>
    <jdk7.artifactId>jdk7</jdk7.artifactId>
    <jdk7.version>1.7.0-u40</jdk7.version>
    <jdk7.classifier>linux-x64</jdk7.classifier>
    <jdk7.type>zip</jdk7.type>

    <jdk6.groupId>com.oracle</jdk6.groupId>
    <jdk6.artifactId>jdk6</jdk6.artifactId>
    <jdk6.version>1.6.0-u45</jdk6.version>
    <jdk6.classifier>linux-x64</jdk6.classifier>
    <jdk6.type>zip</jdk6.type>

  </properties>
  <profiles>
    <profile>
      <id>tomcat6</id>
      <properties>
        <tomcat.version>${tomcat6.version}</tomcat.version>
      </properties>
    </profile>
  </profiles>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>${junit.version}</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.kuali.common</groupId>
      <artifactId>kuali-aws</artifactId>
      <version>${kuali-aws.version}</version>
    </dependency>
    <dependency>
      <groupId>org.kuali.common</groupId>
      <artifactId>kuali-dns</artifactId>
      <version>${kuali-dns.version}</version>
    </dependency>
    <dependency>
      <groupId>org.kuali.common</groupId>
      <artifactId>kuali-util</artifactId>
    </dependency>
    <dependency>
      <groupId>${jdk7.groupId}</groupId>
      <artifactId>${jdk7.artifactId}</artifactId>
      <version>${jdk7.version}</version>
      <classifier>${jdk7.classifier}</classifier>
      <type>${jdk7.type}</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>${jdk6.groupId}</groupId>
      <artifactId>${jdk6.artifactId}</artifactId>
      <version>${jdk6.version}</version>
      <classifier>${jdk6.classifier}</classifier>
      <type>${jdk6.type}</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>${tomcat.groupId}</groupId>
      <artifactId>${tomcat.artifactId}</artifactId>
      <version>${tomcat.version}</version>
      <type>${tomcat.type}</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.kuali.common</groupId>
      <artifactId>kuali-http</artifactId>
      <version>${kuali-http.version}</version>
    </dependency>
    <dependency>
      <groupId>org.kuali.common</groupId>
      <artifactId>kuali-util</artifactId>
      <version>${kuali-util.version}</version>
      <type>test-jar</type>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.hibernate</groupId>
      <artifactId>hibernate-validator</artifactId>
      <version>${hibernate-validator.version}</version>
    </dependency>
    <dependency>
      <groupId>javax.el</groupId>
      <artifactId>javax.el-api</artifactId>
      <version>${javax.el-api.version}</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.glassfish.web</groupId>
      <artifactId>javax.el</artifactId>
      <version>${javax.el.version}</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.google.code.findbugs</groupId>
      <artifactId>jsr305</artifactId>
      <version>${jsr305.version}</version>
      <optional>true</optional>
    </dependency>
  </dependencies>
</project>
