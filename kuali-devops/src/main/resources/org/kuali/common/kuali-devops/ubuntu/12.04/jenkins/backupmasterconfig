function zip_config {
   echo "zip     -> master config"
   cd /home/tomcat7
   rm -f $ZIP
   zip -q -r $ZIP .jenkins -x **/plugins/** **/workspace/** **/logs/slaves/**
}


MY_DIR="$( cd "$( dirname "$0" )" && pwd )"
ME=$(basename $0)
source $MY_DIR/../common/functions.sh

usage() { echo "Usage: $ME [-q --quiet] [-h --help] stack version password secret_key" 1>&2; exit 1; }

STACK=$1
VERSION=$2
GPG_PASSPHRASE=$3
SECRET_KEY=$4

GROUP_ID=org.jenkins
ARTIFACT_ID=jenkins-master-backup
CLASSIFIER=$STACK-$TIMESTAMP
TYPE=zip.gpg

PREFIX=external
ACCESS_KEY=AKIAJFD5IM7IPVVUEBNA
BUCKET=maven.kuali.org

ZIP=/tmp/master.zip
zip_config

