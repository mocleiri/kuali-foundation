<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository>/files/.m2/repository</localRepository>
  <mirrors>
    <mirror>
      <id>nexus.private</id>
      <mirrorOf>*</mirrorOf>
      <url>http://nexus-new.kuali.org/content/groups/developer</url>
    </mirror>
  </mirrors>
  <activeProfiles>
    <activeProfile>kuali-devops</activeProfile>
  </activeProfiles>
  <profiles>
    <profile>
      <id>kuali-devops</id>
      <properties>
        <enc.password>${enc.password}</enc.password>
        <properties.enc.password>${enc.password}</properties.enc.password>
        <kuali.enc.password>${properties.enc.password}</kuali.enc.password>
        <kuali.enc.mode>DECRYPT</kuali.enc.mode>
        <jenkins.username>${jenkins.username}</jenkins.username>
        <dnsme.secretKey>${dnsme.secretKey}</dnsme.secretKey>
        <nexus.kuali.username>${nexus.kuali.username}</nexus.kuali.username>
        <nexus.kuali.password>${nexus.kuali.password}</nexus.kuali.password>
        <nexus.sonatype.username>${nexus.sonatype.username}</nexus.sonatype.username>
        <nexus.sonatype.password>${nexus.sonatype.password}</nexus.sonatype.password>
        <aws.accessKey>${aws.accessKey}</aws.accessKey>
        <aws.secretKey>${aws.secretKey}</aws.secretKey>
      </properties>
    </profile>
  </profiles>
  <servers>
    <server>
      <id>nexus.private</id>
      <username>${nexus.kuali.username}</username>
      <password>${nexus.kuali.password}</password>
    </server>
    <server>
      <id>sonatype-nexus</id>
      <username>${nexus.sonatype.username}</username>
      <password>${nexus.sonatype.password}</password>
    </server>
    <server>
      <id>sonatype-nexus-staging</id>
      <username>${nexus.sonatype.username}</username>
      <password>${nexus.sonatype.password}</password>
    </server>
    <server>
      <id>sonatype-nexus-snapshots</id>
      <username>${nexus.sonatype.username}</username>
      <password>${nexus.sonatype.password}</password>
    </server>
    <server>
      <id>kuali.release</id>
      <username>${aws.accessKey}</username>
      <password>${aws.secretKey}</password>
    </server>
    <server>
      <id>kuali.site</id>
      <username>${aws.accessKey}</username>
      <password>${aws.secretKey}</password>
    </server>
    <server>
      <id>kuali.external</id>
      <username>${aws.accessKey}</username>
      <password>${aws.secretKey}</password>
    </server>
    <server>
      <id>kuali.snapshot</id>
      <username>${aws.accessKey}</username>
      <password>${aws.secretKey}</password>
    </server>
  </servers>
</settings>
