#!/bin/sh -x

#set -x
SEND_MAIL=true; typeset -rx SEND_MAIL
#SEND_MAIL=false; typeset -rx SEND_MAIL

APPLICATION=kra; typeset -rx APPLICATION
JAVA_APPLICATION_CODE=$APPLICATION; typeset -rx JAVA_APPLICATION_CODE
DATABASE_APPLICATION_CODE=$APPLICATION; typeset -rx DATABASE_APPLICATION_CODE
APPLICATION_CFG_MAIL_LIST=gmcgrego@indiana.edu; typeset -rx APPLICATION_CFG_MAIL_LIST
APPLICATION_DEV_MAIL_LIST=gmcgrego@indiana.edu; typeset -rx APPLICATION_DEV_MAIL_LIST
APPLICATION_JAVA_CVS_MODULE=$APPLICATION"_project"; typeset -rx APPLICATION_PROJECT_CVS_MODULE
APPLICATION_DATABASE_CVS_MODULE=$APPLICATION"-cfg-dbs"; typeset -rx APPLICATION_DATABASE_CVS_MODULE