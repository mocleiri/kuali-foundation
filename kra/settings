#!/bin/sh -x

#set -x
SEND_MAIL=true; typeset -rx SEND_MAIL
#SEND_MAIL=false; typeset -rx SEND_MAIL

APPLICATION=kra; typeset -rx APPLICATION
JAVA_APPLICATION_CODE=$APPLICATION; typeset -rx JAVA_APPLICATION_CODE
DATABASE_APPLICATION_CODE=$APPLICATION; typeset -rx DATABASE_APPLICATION_CODE
APPLICATION_CFG_MAIL_LIST=kc.tech.leads@kuali.org; typeset -rx APPLICATION_CFG_MAIL_LIST
APPLICATION_DEV_MAIL_LIST=kc.tech.leads@kuali.org; typeset -rx APPLICATION_DEV_MAIL_LIST
APPLICATION_JAVA_PROJECT="kc_project"; typeset -rx APPLICATION_JAVA_PROJECT
APPLICATION_DATABASE_PROJECT=$APPLICATION"-cfg-dbs"; typeset -rx APPLICATION_DATABASE_PROJECT
APPLICATION_INFRASTRUCTURE_VERSION=branches/rice-release-0-9-4-br; typeset -rx APPLICATION_INFRASTRUCTURE_VERSION
APPLICATION_SERVER_NAME=kuali5; typeset -rx APPLICATION_SERVER_NAME
