#!/bin/sh -x

#set -x
SEND_MAIL=true; typeset -rx SEND_MAIL
#SEND_MAIL=false; typeset -rx SEND_MAIL

APPLICATION=kfs; typeset -rx APPLICATION
JAVA_APPLICATION_CODE=kfs; typeset -rx JAVA_APPLICATION_CODE
DATABASE_APPLICATION_CODE=kul; typeset -rx DATABASE_APPLICATION_CODE
APPLICATION_CFG_MAIL_LIST=kfcm-l@indiana.edu; typeset -rx APPLICATION_CFG_MAIL_LIST
APPLICATION_DEV_MAIL_LIST=kftdiscuss-l@indiana.edu; typeset -rx APPLICATION_DEV_MAIL_LIST
APPLICATION_JAVA_PROJECT=$APPLICATION; typeset -rx APPLICATION_JAVA_PROJECT
APPLICATION_DATABASE_PROJECT=$APPLICATION"-cfg-dbs"; typeset -rx APPLICATION_DATABASE_PROJECT
APPLICATION_INFRASTRUCTURE_VERSION=branches/rice-release-1-0-1-br; typeset -rx APPLICATION_INFRASTRUCTURE_VERSION
APPLICATION_SERVER_NAME=kuali2; typeset -rx APPLICATION_SERVER_NAME