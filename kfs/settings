#!/bin/sh -x

#set -x
#SEND_MAIL=true; typeset -rx SEND_MAIL
SEND_MAIL=false; typeset -rx SEND_MAIL

APPLICATION=kfs; typeset -rx APPLICATION
JAVA_APPLICATION_CODE=kuali; typeset -rx JAVA_APPLICATION_CODE
DATABASE_APPLICATION_CODE=kul; typeset -rx DATABASE_APPLICATION_CODE
APPLICATION_CFG_MAIL_LIST=kfcm-l@indiana.edu; typeset -rx APPLICATION_CFG_MAIL_LIST
APPLICATION_DEV_MAIL_LIST=kftdiscuss-l@indiana.edu; typeset -rx APPLICATION_DEV_MAIL_LIST
APPLICATION_JAVA_CVS_MODULE=$APPLICATION_CODE"_project"; typeset -rx APPLICATION_PROJECT_CVS_MODULE
APPLICATION_DATABASE_CVS_MODULE=$APPLICATION"-cfg-dbs"; typeset -rx APPLICATION_DATABASE_CVS_MODULE