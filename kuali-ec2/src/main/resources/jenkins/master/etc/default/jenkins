NAME=jenkins
JAVA=/var/lib/jenkins/tools/jdk7/bin/java
JAVA_ARGS="-Xmx2g -XX:MaxPermSize=256m -Duser.timezone=America/New_York"
PIDFILE=/var/run/jenkins/jenkins.pid
JENKINS_USER=jenkins
JENKINS_WAR=/usr/share/jenkins/jenkins.war
JENKINS_HOME=/var/lib/jenkins
RUN_STANDALONE=true
JENKINS_LOG=/var/log/jenkins/$NAME.log
MAXOPENFILES=8192
HTTP_PORT=8080
AJP_PORT=-1
JENKINS_ARGS="--webappsDir=/var/cache/winstone/webapps --httpPort=$HTTP_PORT --ajp13Port=$AJP_PORT"
