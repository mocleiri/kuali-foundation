<data xmlns="ns:workflow" xsi:schemaLocation="ns:workflow resource:WorkflowData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>OLE_CTRL</name>
      <parent>OLEAccountsReceivableTransactionalDocument</parent>
      <label>
        Cash Control
      </label>
      <docHandler>
        ${application.url}/arCashControlDocument.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fcashcontrol.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcashcontrols.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Lockbox"/>
          <role name="Lockbox" nextNode="AssociatedWithElectronicPayment"/>
          <split name="AssociatedWithElectronicPayment">
            <branch name="True">
              <role name="ElectronicPayment" nextNode="Join"/>
            </branch>
            <branch name="False">
              <simple name="NoOp" nextNode="Join"/>
            </branch>
            <join name="Join"/>
          </split>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="Lockbox">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
          <mandatoryRoute>
            true
          </mandatoryRoute>
        </role>
        <split name="AssociatedWithElectronicPayment">
          <type>
            org.kuali.ole.sys.document.workflow.SimpleBooleanSplitNode
          </type>
        </split>
        <role name="ElectronicPayment">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
        <join name="Join"/>
        <simple name="NoOp">
          <type>
            org.kuali.rice.kew.engine.node.NoOpNode
          </type>
        </simple>
      </routeNodes>
    </documentType>
    <documentType>
      <name>OLE_APP</name>
      <parent>OLEAccountsReceivableTransactionalDocument</parent>
      <label>
        Application
      </label>
      <docHandler>
        ${application.url}/arPaymentApplicationDocument.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fpaymentapplication.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fpaymentapplications.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
    </documentType>
    <documentType>
      <name>OLE_INVW</name>
      <parent>OLEAccountsReceivableTransactionalDocument</parent>
      <label>
        Customer Invoice Writeoff
      </label>
      <docHandler>
        ${application.url}/arCustomerInvoiceWriteoffDocument.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fcustomerinvoicewriteoff.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomerinvoicewriteoffscustomdocumentsearch.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="RequiresApproval"/>
          <split name="RequiresApproval">
            <branch name="True">
              <role name="Account" nextNode="Join"/>
            </branch>
            <branch name="False">
              <simple name="NoOp" nextNode="Join"/>
            </branch>
            <join name="Join"/>
          </split>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <split name="RequiresApproval">
          <type>
            org.kuali.ole.sys.document.workflow.SimpleBooleanSplitNode
          </type>
        </split>
        <role name="Account">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
          <activationType>
            P
          </activationType>
        </role>
        <join name="Join"/>
        <simple name="NoOp">
          <type>
            org.kuali.rice.kew.engine.node.NoOpNode
          </type>
        </simple>
      </routeNodes>
    </documentType>
    <documentType>
      <name>OLE_CRM</name>
      <parent>OLEAccountsReceivableTransactionalDocument</parent>
      <label>
        Customer Credit Memo
      </label>
      <docHandler>
        ${application.url}/arCustomerCreditMemoDocument.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fcustomercreditmemo.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomercreditmemos.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Account"/>
          <role name="Account"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="Account">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
          <activationType>
            P
          </activationType>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>OLE_INV</name>
      <parent>OLEAccountsReceivableTransactionalDocument</parent>
      <label>
        Customer Invoice
      </label>
      <docHandler>
        ${application.url}/arCustomerInvoiceDocument.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fcustomerinvoice.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomerinvoices.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="HasReccurence"/>
          <split name="HasReccurence">
            <branch name="True">
              <role name="Account" nextNode="Recurrence"/>
              <role name="Recurrence" nextNode="Join"/>
            </branch>
            <branch name="False">
              <simple name="NoOp" nextNode="Join"/>
            </branch>
            <join name="Join"/>
          </split>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <split name="HasReccurence">
          <type>
            org.kuali.ole.sys.document.workflow.SimpleBooleanSplitNode
          </type>
        </split>
        <role name="Account">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
          <activationType>
            P
          </activationType>
        </role>
        <role name="Recurrence">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
          <activationType>
            P
          </activationType>
        </role>
        <join name="Join"/>
        <simple name="NoOp">
          <type>
            org.kuali.rice.kew.engine.node.NoOpNode
          </type>
        </simple>
      </routeNodes>
    </documentType>
  </documentTypes>
</data>
