<data xmlns="ns:workflow" xsi:schemaLocation="ns:workflow resource:WorkflowData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
     <documentType>
      <name>OLE_DFTTABCL</name>
      <parent>OLESelectTransactionalDocument</parent>
      <label>Default Table Column Value</label>
    </documentType>
    
    <documentType>
      <name>OLE_ACQBTHUPLOAD</name>
      <parent>OLESelectTransactionalDocument</parent>
      <label>
        Acquisition Batch Upload
      </label>
      <docHandler>
        ${application.url}/acqBatchUpload.do?methodToCall=docHandler
      </docHandler>
      <helpDefinitionURL>
        default.htm?turl=WordDocuments%2Fpurchaseorder.htm
      </helpDefinitionURL>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Fcustomdocumentsearches.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="Approval"/>
          <role name="Approval"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="Approval">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>OLE_QUEUESEARCH</name>
      <parent>OLESelectTransactionalDocument</parent>
      <label>Receiving Queue Search</label>
    </documentType>

	<documentType>
		<name>OLE_ORDQU</name>
		<parent>OLESelectTransactionalDocument</parent>
		<label>
			OrderQueueDocument
		</label>
		<active>
			true
		</active>
		<routingVersion>
			2
		</routingVersion>
	</documentType>
	<documentType>
		<name>OLE_ACQS</name>
		<parent>OLESelectTransactionalDocument</parent>
		<label>
			Acquisitions Search
		</label>
		<active>
			true
		</active>
		<routingVersion>
			2
		</routingVersion>
	</documentType>        

  </documentTypes>
</data>
