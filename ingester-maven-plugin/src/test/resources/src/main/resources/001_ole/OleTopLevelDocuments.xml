<data xmlns="ns:workflow" xsi:schemaLocation="ns:workflow resource:WorkflowData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ruleAttributes xmlns="ns:workflow/RuleAttribute" xsi:schemaLocation="ns:workflow/RuleAttribute resource:RuleAttribute">
    <ruleAttribute>
      <name>OpenLibraryEnvironmentSearchableAttribute</name>
      <className>org.kuali.ole.sys.document.workflow.OpenLibraryEnvironmentSearchableAttribute</className>
      <label>Open Library Environment Searchable Attribute</label>
      <description>Open Library Environment Searchable Attribute</description>
      <type>SearchableAttribute</type>
    </ruleAttribute>
    <ruleAttribute>
      <name>OpenLibraryEnvironmentDocumentSearchResultProcessorAttribute</name>
      <className>org.kuali.ole.sys.document.workflow.DataDictionaryDocumentSearchCustomizer</className>
      <label>Open Library Environment Document Search Result Processor Attribute</label>
      <description>Open Library Environment Document Search Result Processor Attribute</description>
      <type>DocumentSearchResultProcessorAttribute</type>
    </ruleAttribute>
    <ruleAttribute>
      <name>OpenLibraryEnvironmentDocumentSearchGeneratorAttribute</name>
      <className>org.kuali.ole.sys.document.workflow.DataDictionaryDocumentSearchCustomizer</className>
      <label>Open Library Environment Document Search Generator Attribute</label>
      <description>Open Library Environment Document Search Generator Attribute</description>
      <type>DocumentSearchGeneratorAttribute</type>
    </ruleAttribute>
    <ruleAttribute>
      <name>NoFyiActionListAttribute</name>
      <className>org.kuali.ole.module.purap.document.workflow.PurchaseOrderActionListAttribute</className>
      <label>No FYI</label>
      <description>No FYI Action List Attribute</description>
      <type>ActionListAttribute</type>
    </ruleAttribute>
  </ruleAttributes>
  <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
    <documentType>
      <name>
        OLE
      </name>
      <parent>
        KualiDocument
      </parent>
      <label>
        OpenLibraryEnvironmentDocument
      </label>
      <serviceNamespace>
        OLE
      </serviceNamespace>
      <docHandler>
        ${application.url}/kr/maintenance.do?methodToCall=docHandler
      </docHandler>
      <postProcessorName>
        org.kuali.ole.sys.document.workflow.PostProcessor
      </postProcessorName>
      <active>
        true
      </active>
      <attributes>
        <attribute>
          <name>
            OpenLibraryEnvironmentSearchableAttribute
          </name>
        </attribute>
        <attribute>
          <name>
            OpenLibraryEnvironmentDocumentSearchResultProcessorAttribute
          </name>
        </attribute>
        <attribute>
          <name>
            OpenLibraryEnvironmentDocumentSearchGeneratorAttribute
          </name>
        </attribute>
      </attributes>
      <routingVersion>
        2
      </routingVersion>
    </documentType>
    <documentType>
      <name>
        OpenLibraryEnvironmentSimpleMaintenanceDocument
      </name>
      <parent>
        OLE
      </parent>
      <label>
        OpenLibraryEnvironmentSimpleMaintenanceDocument
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        OpenLibraryEnvironmentComplexMaintenanceDocument
      </name>
      <parent>
        OLE
      </parent>
      <label>
        OpenLibraryEnvironmentComplexMaintenanceDocument
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths>
        <routePath>
          <start name="AdHoc" nextNode="OrganizationHierarchy"/>
          <role name="OrganizationHierarchy"/>
        </routePath>
      </routePaths>
      <routeNodes>
        <start name="AdHoc"/>
        <role name="OrganizationHierarchy">
          <qualifierResolverClass>
            org.kuali.rice.kns.workflow.attribute.DataDictionaryQualifierResolver
          </qualifierResolverClass>
        </role>
      </routeNodes>
    </documentType>
    <documentType>
      <name>
        OpenLibraryEnvironmentTransactionalDocument
      </name>
      <parent>
        OLE
      </parent>
      <label>
        OpenLibraryEnvironmentTransactionalDocument
      </label>
      <docSearchHelpURL>
        default.htm?turl=WordDocuments%2Ffinancialtransactions.htm
      </docSearchHelpURL>
      <active>
        true
      </active>
      <security>
        <securityAttribute class="org.kuali.ole.sys.document.workflow.SensitiveDataSecurityAttribute"/>
      </security>
      <routingVersion>
        2
      </routingVersion>
    </documentType>
    <documentType>
      <name>
        OpenLibraryEnvironmentLedgerOnlyDocument
      </name>
      <label>
        Open Library Environment - Ledger Only
      </label>
      <active>
        true
      </active>
      <routingVersion>
        2
      </routingVersion>
      <routePaths/>
    </documentType>
  </documentTypes>
</data>
