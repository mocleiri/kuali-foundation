#!/bin/sh -x

SAMPLEAPP_SRCCTRL_URL=$SRCCTRL_URL/rice-sample-apps
JAVA_APPLICATION_CODE=recipe; typeset -rx JAVA_APPLICATION_CODE
DATABASE_APPLICATION_CODE=rice094; typeset -x DATABASE_APPLICATION_CODE
