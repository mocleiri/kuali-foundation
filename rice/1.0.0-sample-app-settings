APPLICATION_INFRASTRUCTURE_VERSION=branches/rice-release-1-0-0-br; typeset -rx APPLICATION_INFRASTRUCTURE_VERSION
APPLICATION_SERVER_NAME=kuali14; typeset -rx APPLICATION_SERVER_NAME